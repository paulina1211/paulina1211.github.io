        "karma-jasmine": "3.1.1",
        "karma-jasmine-html-reporter": "1.5.2",
        "prettier": "1.19.1",
        "protractor": "5.4.3",
        "pug": "2.0.4",
        "pug-lint": "2.6.0",
        "shelljs": "0.8.3",
        "ts-node": "8.6.2",
        "tslint": "6.0.0",
        "tslint-plugin-prettier": "2.1.0",
        "typescript": "3.7.5"
    }
}
